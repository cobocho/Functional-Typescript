{
  "extends": [
    "@titicaca/eslint-config-triple",
    "@titicaca/eslint-config-triple/requiring-type-checking",
    "@titicaca/eslint-config-triple/prettier",
  ],
  "env": {
    "es2021": true,
    "node": true,
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "no-console": "off",
    "no-var": "off",
  },
}
